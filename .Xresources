*dpi: 210

Xft.dpi: 210
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

! special
*.foreground:   #ffffff
*.background:   #000000
*.cursorColor:  #ffffff

! black
*.color0:       #25283d
*.color8:       #709080

! red
*.color1:       #edcf8e
*.color9:       #ee2e31

! green
*.color2:       #07bee8
*.color10:      #e83f6f

! yellow
*.color3:       #84dcc6
*.color11:      #f0dfaf

! blue
*.color4:       #0a8754
*.color12:      #94bff3

! magenta
*.color5:       #af3b6e
*.color13:      #ffbf00

! cyan
*.color6:       #cbf3f0
*.color14:      #1cfeba

! white
*.color7:       #edddd4
*.color15:      #ffffff

*.font: xft:Iosevka:style=Regular:size=13
*.boldFont: xft:Iosevka:style=Bold:size=13
*.italicFont: xft:Iosevka:style=Italic:size=13
*.boldItalicFont: xft:Iosevka:style=Bold Italic:size=13

Xmessage*defaultButton:            okay
Xmessage*form.message.Scroll:      WhenNeeded
